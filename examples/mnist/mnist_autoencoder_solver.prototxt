net: "examples/mnist/mnist_autoencoder.prototxt"
test_state: { stage: 'test-on-train' }
test_iter: 1
test_state: { stage: 'test-on-test' }
test_iter: 1
test_interval: 500
test_compute_loss: false
base_lr: 0.01
lr_policy: "step"
gamma: 0.1
stepsize: 10000
display: 1
max_iter: 5
weight_decay: 0.0005
snapshot: 10000
snapshot_prefix: "examples/mnist/mnist_autoencoder"
momentum: 0.9
random_seed: 1701
# solver mode: CPU or GPU
solver_mode: CPU
